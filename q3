list1=['Rajesh','Suresh','Sam','Rajesh','Manish']
list2=['21','28','26','21','29']
list3=['london','California','Delhi','Gurgaon','Banglore']
old_data=zip(list1,list2,list3)
#print(old_data)
print(set(old_data))
dict={}

for i in range(len(list1)):
    dict.update({str(list1[i])+' '+str(list2[i]):str(list3[i])})
    
print("Updated Dict is: ", dict)
